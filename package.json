{
  "name": "treegen",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite --open",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "code:style:check": "prettier --check .",
    "code:style:fix": "prettier --write .",
    "code:lint:check": "eslint --ext .js,.ts,.tsx .",
    "code:lint:fix": "eslint --fix --ext .js,.ts,.tsx .",
    "prepare": "husky install"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rxjs": "7.8.0"
  },
  "devDependencies": {
    "@types/node": "18.14.5",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "5.54.0",
    "@typescript-eslint/parser": "5.54.0",
    "@vitejs/plugin-react-swc": "3.2.0",
    "eslint": "8.35.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-react": "7.32.2",
    "husky": "8.0.3",
    "lint-staged": "13.1.2",
    "prettier": "2.8.4",
    "typescript": "4.9.5",
    "vite": "4.1.4",
    "vite-plugin-checker": "0.5.6",
    "vitest": "0.29.2"
  }
}
